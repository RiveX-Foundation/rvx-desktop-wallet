  "scripts": {
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "start": "electron .",
    "dev": "npm run dev:renderer",
    "dev:main": "cross-env NODE_ENV=development electron -r @babel/register ./src/main.dev.js ",
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./config/webpack/webpack.config.renderer.babel.js --colors",
    "transpile:renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.renderer.prod.babel.js --colors",
    "transpile:main": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.main.prod.babel.js --colors",
    "pack:mac": "run-s copy:mac transpile:renderer transpile:main build:mac",
    "pack:win": "run-s copy:win transpile:renderer transpile:main build:win",
    "pack:linux": "run-s copy transpile:renderer transpile:main build:linux",
    "build:mac": "sudo electron-builder --mac -p never",
    "build:win": "electron-builder --win -p never",
    "build:linux": "electron-builder --linux -p never",
    "build:all": "electron-builder -mwl -p never",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy": "./build.sh",
    "copy:mac": "./build-mac.sh",
    "copy:win": "build.bat",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RiveX-Foundation/rx-wallet-desktop"
